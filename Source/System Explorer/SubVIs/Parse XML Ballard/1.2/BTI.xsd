            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="Zeroes" />
                    <xs:enumeration value="123" />
                    <xs:enumeration value="CWD" />
                    <xs:enumeration value="None" />
                </xs:restriction>
            </xs:simpleType>
