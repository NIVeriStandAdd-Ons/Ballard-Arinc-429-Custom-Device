                    <xs:enumeration value="PPS1" />
                    <xs:enumeration value="PWMIRIG0" />
                    <xs:enumeration value="PWMIRIG1" />
                    <xs:enumeration value="AMIRIG" />
                    <xs:enumeration value="10MHZ" />
                    <xs:enumeration value="HOST" />
